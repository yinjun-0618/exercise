<?xml version="1.0" encoding="utf-8"?>
<DirectionalLayout
    xmlns:ohos="http://schemas.huawei.com/res/ohos"
    ohos:height="match_parent"
    ohos:width="match_parent"
    ohos:orientation="vertical">

    <DatePicker
        ohos:id="$+id:date_pick"
        ohos:height="match_content"
        ohos:width="match_parent"
        ohos:text_size="20fp"
        ohos:selector_item_num="2"
        ohos:background_element="#C89FDEFF">
    </DatePicker>


    <DirectionalLayout
        ohos:top_margin="15vp"
        ohos:height="40vp"
        ohos:orientation="horizontal"
        ohos:width="match_parent">

        <Text
            ohos:height="match_parent"
            ohos:width="80vp"
            ohos:text="卧推"
            ohos:text_size="15fp"></Text>


        <Text
            ohos:height="match_parent"
            ohos:width="80vp"
            ohos:text="1分30秒"
            ohos:text_size="15fp"></Text>

        <Button
            ohos:height="match_parent"
            ohos:width="80vp"
            ohos:text="评价"
            ohos:background_element="$graphic:background_button_common"
            ohos:text_size="15fp">
        </Button>

        <Button
            ohos:left_margin="20vp"
            ohos:height="match_parent"
            ohos:width="80vp"
            ohos:text="查看"
            ohos:background_element="$graphic:background_button_common"
            ohos:text_size="15fp">
        </Button>

